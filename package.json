    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "run-p 'lint:* -- --fix'",
    "lint:js": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.json",
    "lint:css": "stylelint '**/*.{css,vue}'",
    "ci:lint": "run-p lint:*",
    "audit": "npm audit --omit dev"
  },
  "dependencies": {
    "raf-throttle": "2.0.5",
    "vue": "3.2.41",
    "webapps-common": "file:webapps-common"
  },
  "devDependencies": {
    "@knime/eslint-config": "7.0.5",
    "@types/node": "18.11.2",
    "@vitejs/plugin-vue": "3.1.2",
    "@vue/tsconfig": "0.1.3",
    "eslint": "8.25.0",
    "npm-run-all": "4.1.5",
    "postcss-import": "15.0.0",
    "postcss-preset-env": "7.8.2",
    "postcss-url": "10.1.3",
    "stylelint": "14.14.0",
    "typescript": "4.8.4",
    "vite": "3.1.8",
    "vite-svg-loader": "3.6.0",
    "vite-plugin-css-injected-by-js": "^2.1.1",
    "vue-tsc": "1.0.8"
  },
  "files": [
    "dist"
  ],
  "module": "./dist/KNIME-Table.js",
  "exports": {
    ".": {
      "import": "./dist/KNIME-Table.js"
