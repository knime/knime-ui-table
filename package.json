{
  "name": "@knime/knime-ui-table",
  "type": "module",
  "version": "0.3.69",
  "description": "Data Table for KNIME",
  "author": "KNIME AG, Zurich, Switzerland",
  "license": "See the file license.txt",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc -p tsconfig.vitest.json --noEmit",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check .",
    "lint": "run-p 'lint:* -- --fix'",
    "lint:js": "eslint . --cache --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:css": "stylelint --cache '**/*.{css,vue}'",
    "ci:lint-format": "run-p ci:lint:* format:check -c",
    "ci:lint:js": "npm run lint:js && npm run lint:js -- -f json -o test-results/eslint.json",
    "ci:lint:css": "npm run lint:css -- -f json -o test-results/stylelint.json",
    "audit": "npm audit --omit dev",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "storybook": "sb dev",
    "prepare": "husky"
  },
  "dependencies": {
    "raf-throttle": "2.0.6",
    "uuid": "9.0.1",
    "vue": "3.4.21",
    "vue-virtual-scroller": "file:vue-virtual-scroller",
    "webapps-common": "file:webapps-common"
  },
  "devDependencies": {
    "@knime/eslint-config": "8.0.11",
    "@storybook/addon-actions": "7.6.17",
    "@storybook/addon-essentials": "7.6.17",
    "@storybook/addon-interactions": "7.6.17",
    "@storybook/addon-links": "7.6.17",
    "@storybook/testing-library": "0.2.2",
    "@storybook/vue3-vite": "7.6.17",
    "@tsconfig/node20": "20.1.2",
    "@types/jsdom": "21.1.6",
    "@types/node": "20.11.25",
    "@types/uuid": "9.0.8",
    "@vitejs/plugin-vue": "5.0.3",
    "@vitest/coverage-v8": "1.3.1",
    "@vue/test-utils": "2.4.4",
    "@vue/tsconfig": "0.5.1",
    "consola": "2.15.3",
    "eslint": "8.57.0",
    "husky": "9.0.11",
    "jsdom": "24.0.0",
    "npm-run-all": "4.1.5",
    "postcss-import": "16.0.1",
    "postcss-preset-env": "9.5.0",
    "postcss-url": "10.1.3",
    "prettier": "3.2.5",
    "storybook": "7.6.17",
    "stylelint": "16.2.1",
    "typescript": "5.3.3",
    "vite": "5.1.5",
    "vite-dts": "1.0.4",
    "vite-plugin-dts": "3.7.3",
    "vite-plugin-lib-inject-css": "2.0.0",
    "vite-svg-loader": "5.1.0",
    "vitest": "1.3.1",
    "vue-tsc": "2.0.6"
  },
  "peerDependencies": {
    "vue": "3.x"
  },
  "files": [
    "dist",
    "util/constants.js"
  ],
  "module": "./dist/knime-ui-table.js",
  "types": "./dist/knime-ui-table.d.ts",
  "exports": "./dist/knime-ui-table.js",
  "engines": {
    "node": "20.x"
  },
  "volta": {
    "node": "20.11.1"
  },
  "sideEffects": [
    "dist/*.css"
  ]
}
