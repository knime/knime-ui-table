{
  "name": "@knime/ui-table",
  "version": "0.2.0",
  "description": "Data Table for KNIME",
  "author": "KNIME AG, Zurich, Switzerland",
  "license": "See the file LICENSE",
  "scripts": {
    "dev": "vite --port 4000",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "run-p lint:*",
    "lint:js": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:css": "stylelint '**/*.{css,vue}'"
  },
  "dependencies": {
    "raf-throttle": "2.0.5",
    "vue": "3.2.41",
    "webapps-common": "file:webapps-common"
  },
  "devDependencies": {
    "@knime/eslint-config": "7.0.5",
    "@types/node": "^18.8.5",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "8.23.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "15.0.0",
    "postcss-preset-env": "7.8.2",
    "postcss-url": "10.1.3",
    "stylelint": "^14.11.0",
    "typescript": "~4.7.4",
    "vite": "^3.0.4",
    "vite-svg-loader": "3.5.1",
    "vue-tsc": "^0.39.5"
  },
  "files": ["dist"],
  "main": "./dist/KNIME-Table.umd.cjs",
  "module": "./dist/KNIME-Table.js",
  "exports": {
    ".": {
      "import": "./dist/KNIME-Table.js",
      "require": "./dist/KNIME-Table.umd.cjs"
    }
  },
  "engines": {
    "node": "18.x"
  },
  "volta": {
    "node": "18.10.0"
  }
}
