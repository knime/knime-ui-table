{
  "name": "@knime/knime-ui-table",
  "version": "0.1.29",
  "description": "JS Data Table for KNIME",
  "scripts": {
    "audit": "check-audit --production",
    "build": "vue-cli-service build --target lib --name KNIME-Table index.js",
    "build:report": "npm run build -- --report",
    "coverage": "npm run test:unit -- --coverage",
    "demo": "ESLINT_NO_DEV_ERRORS='true' vue serve demo/Demo.vue",
    "inspect": "vue inspect",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext .js,.vue .",
    "lint:css": "stylelint '**/*.{css,vue}'",
    "opensourcecredits": "npm run --prefix webapps-common/ opensourcecredits",
    "test:unit": "jest",
    "sendcoverage": "cross-var sonar-scanner -Dsonar.login=\"$SONAR_LOGIN\" -Dsonar.password=\"$SONAR_PASSWORD\""
  },
  "license": "See the file LICENSE",
  "author": "KNIME AG, Zurich, Switzerland",
  "main": "dist/KNIME-Table.common.js",
  "files": [
    "dist/KNIME-Table.*",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "vue-clickaway": "2.2.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.4",
    "@vue/cli-service": "4.5.4",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "consola": "2.10.1",
    "core-js": "3.6.5",
    "cross-var": "1.1.0",
    "eslint": "5.16.0",
    "eslint-plugin-vue": "7.10.0",
    "eslint-plugin-jest-formatting": "3.0.0",
    "fontsource-roboto": "3.0.3",
    "fontsource-roboto-mono": "3.0.3",
    "jest": "24.9.0",
    "jest-junit": "7.0.0",
    "jest-transform-stub": "2.0.0",
    "modern-normalize": "1.1.0",
    "npm-audit-resolver": "2.3.1",
    "postcss-import": "12.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-url": "8.0.0",
    "raw-loader": "3.1.0",
    "sonar-scanner": "3.1.0",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "20.0.0",
    "svgo-loader": "2.2.1",
    "vue": "2.6.*",
    "vue-jest": "3.0.4",
    "vue-svg-loader": "0.16.0",
    "vue-template-compiler": "2.6.11",
    "webapps-common": "file:webapps-common"
  },
  "peerDependencies": {
    "vue": "2.6.*",
    "consola": "2.*",
    "fontsource-roboto": "~3.*",
    "fontsource-roboto-mono": "~3.*"
  },
  "engines": {
    "node": "14.x"
  }
}
