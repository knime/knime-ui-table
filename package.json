  "dependencies": {
    "raf-throttle": "2.0.6",
    "vue3-click-away": "1.2.4",
    "vue-virtual-scroller": "file:vue-virtual-scroller",
    "webapps-common": "file:webapps-common"
  },
  "devDependencies": {
    "@knime/eslint-config": "7.1.4",
    "@types/jsdom": "20.0.1",
    "@types/node": "18.14.2",
    "@vitejs/plugin-vue": "4.0.0",
    "@vitest/coverage-c8": "0.29.2",
    "@vue/test-utils": "2.2.6",
    "@vue/tsconfig": "0.1.3",
    "eslint": "8.35.0",
    "jsdom": "20.0.3",
    "npm-run-all": "4.1.5",
