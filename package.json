{
  "name": "knime-data-table",
  "version": "0.1.0",
  "description": "JS Data Table for KNIME",
  "private": true,
  "scripts": {
    "demo": "vue serve demo/App.vue",
    "build": "vue-cli-service build --no-clean --target lib --formats umd-min src/components/Table.vue",
    "build:dev": "npm run build -- --watch",
    "dev-inte": "concurrently -n server,app -c yellow,blue \"http-server -c-1 --cors -p 4000 dist/ http-server\" \"npm run build:dev\"",
    "inspect": "vue inspect",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext .js,.vue .",
    "lint:css": "stylelint '**/*.{css,vue}'"
  },
  "license": "UNLICENSED",
  "author": "KNIME AG, Zurich, Switzerland",
  "dependencies": {
    "consola": "2.10.1",
    "core-js": "3.6.5",
    "normalize.css": "8.0.1",
    "fontsource-roboto": "3.0.3",
    "fontsource-roboto-mono": "3.0.3",
    "vue": "2.6.11",
    "vuex": "3.0.1",
    "vue-clickaway": "2.2.2",
    "webapps-common": "file:webapps-common"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.4",
    "@vue/cli-service": "4.5.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "concurrently": "5.3.0",
    "cross-env": "5.2.0",
    "cross-var": "1.1.0",
    "eslint": "5.16.0",
    "eslint-plugin-vue": "7.10.0",
    "eslint-plugin-jest-formatting": "3.0.0",
    "http-server": "0.12.3",
    "postcss-css-variables": "0.13.0",
    "postcss-import": "12.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-url": "8.0.0",
    "raw-loader": "3.1.0",
    "sonar-scanner": "3.1.0",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "20.0.0",
    "svgo-loader": "2.2.1",
    "vue-svg-loader": "0.11.0",
    "vue-template-compiler": "2.6.11"
  },
  "engines": {
    "node": "14.x"
  }
}
