{
  "name": "@knime/knime-ui-table",
  "type": "module",
  "version": "0.3.0",
  "description": "Data Table for KNIME",
  "author": "KNIME AG, Zurich, Switzerland",
  "license": "See the file license.txt",
  "scripts": {
    "postinstall": "npm --prefix vue-virtual-scroller run build",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "run-p 'lint:* -- --fix'",
    "lint:js": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.json",
    "lint:css": "stylelint '**/*.{css,vue}'",
    "ci:lint": "run-p lint:*",
    "audit": "npm audit --omit dev",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@popperjs/core": "2.5.4",
    "raf-throttle": "2.0.5",
    "vue": "3.2.45",
    "vue3-click-away": "1.2.4",
    "vue-virtual-scroller": "file:vue-virtual-scroller",
    "webapps-common": "file:webapps-common"
  },
  "devDependencies": {
    "@knime/eslint-config": "7.0.6",
    "@types/jsdom": "20.0.1",
    "@types/node": "18.11.11",
    "@vitejs/plugin-vue": "3.2.0",
    "@vitest/coverage-c8": "0.25.4",
    "@vue/test-utils": "2.2.6",
    "@vue/tsconfig": "0.1.3",
    "eslint": "8.29.0",
    "jsdom": "20.0.3",
    "npm-run-all": "4.1.5",
    "postcss-import": "15.0.1",
    "postcss-preset-env": "7.8.3",
    "postcss-url": "10.1.3",
    "stylelint": "14.16.0",
    "typescript": "4.9.3",
    "vite": "3.2.5",
    "consola": "2.15.3",
    "vite-plugin-css-injected-by-js": "2.1.1",
    "vite-svg-loader": "3.6.0",
    "vitest": "0.25.4",
    "vue-tsc": "1.0.11"
  },
  "files": [
    "dist"
  ],
  "module": "./dist/knime-ui-table.js",
  "exports": {
    ".": {
      "import": "./dist/knime-ui-table.js"
    }
  },
  "engines": {
    "node": "18.x"
  },
  "volta": {
    "node": "18.12.1"
  }
}
